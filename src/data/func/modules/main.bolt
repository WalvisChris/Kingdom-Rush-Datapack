function func:update:
    # towers
    execute as @e[tag=buildspot] run function func:tower/update
    # enemies
    execute as @e[tag=enemy] run function func:enemy/update
    # GUI
    execute as @e[tag=gui] run function func:gui/update
    # soldier
    execute as @e[tag=soldier] run function func:soldier/update
    # bomb placeholder
    execute as @e[tag=bomb.placeholder] run function func:tower/bomb/placeholder